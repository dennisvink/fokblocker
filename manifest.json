{
  "manifest_version": 2,
  "name": "FOKBlocker Chrome",
  "version": "0.1.0",
  "background": {
    "scripts": ["content.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://forum.fok.nl/*",
        "http://forum.fok.nl/*"
      ],
      "js": ["jquery.min.js","content.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icon_128.png"
  }
}
